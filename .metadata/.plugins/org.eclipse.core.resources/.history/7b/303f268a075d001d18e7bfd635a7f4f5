
public class Trial {
	public static void main(String[] args) {
		int num=10;
		switch(num) {
		
		case 10:
			int output=num*25;
			System.out.println(output);
			break;
			
		case 20:
			System.out.println("You are the right candidate for the job");
			break;
			
		case 30:
			System.out.println("You are the right candidate for the job");
			break;
			
		case 12:
			System.out.println("You are the right candidate for the job");
			break;
			
		case 14:
			System.out.println("You are the right candidate for the job");
			break;
		
		
		
		
		
		
		
		
		}
		
		
	
	}
}

